<%- include("partials/header") -%>
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label for="ptitl">Title</label>
            <input type="text" name="pTitle" id="ptitl" class="form-control">
            <label for="pcontent">Post</label>
            <textarea name="pcontent" id="pcontent" cols="100" rows="10" class="form-control"></textarea>
        </div>
        <button type="submit" name="button" class="btn btn-primary">Publish</button>
    </form>
    <%- include("partials/footer") -%>